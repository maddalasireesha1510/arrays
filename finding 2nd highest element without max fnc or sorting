#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n,j,x,m=0;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    m=a[0];
    for(int i=1;i<n;i++)
    {
        if(a[i]>m)
        {
            m=a[i];
        }
    }
    cout<<m<<endl;
    j=1;
    while(true)
    {
        for(int i=0;i<n;i++)
        {
            x=0;
            if(a[i]==m-j)
            {
                cout<<a[i];
                x=1;
                break;
            }
        }
        if(x==1)
        {
            break;
        }
        j++;    
    }
	return 0;
}
