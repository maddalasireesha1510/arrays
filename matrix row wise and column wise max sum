n=int(input())
m=int(input())
matrix=[]
l=[]
for i in range(n):
    a=[]
    for j in range(m):
        a.append(int(input()))
    matrix.append(a)


    
for i in range(n):
    c=0
    for j in range(m):
        c+=matrix[i][j]
    l.append(c)
for i in range(n):
    d=0
    for j in range(m):
        d+=matrix[j][i]
    l.append(d)
print(max(l))
